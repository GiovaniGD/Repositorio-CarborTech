                <input type="text" id="searchInput" name="search" placeholder="Pesquise aqui">
                <input type="submit" value="Buscar">
            </form>
        </div>

        <div class="buttons">
            <a href="/login"><button class="btn-entrar">Login</button></a>
            <a href="/cadastro"><button class="btn-cadastrar-se">Cadastrar-se</button></a>
        </div>
